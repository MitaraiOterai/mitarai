return 0;}
	
	float x1,x2;
	x1=((-b)-sqrt(delta))/2*a;
	x2=((-b)+sqrt(delta))/2*a;
	cout<<"该方程的两个解分别为："<<x1<<"和"<<x2<<".";
	system("pause");
