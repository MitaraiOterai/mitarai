#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	float a,b,c,delta;
	cout <<"请输入二次项：";
	cin>>a;
	cout<<"请输入一次项：";
	cin>>b;
	cout<<"请输入常数项：";
	cin>>c; 
	delta=(b*b)-(4*a*c);
	if(delta<0){
 
cout<<"	  . . . D * * * * W L : . . .   . . : j D E D D D G t ,     . : . . . . .     : : . .         	"<<endl;
